  Legend:
    Name: "\u0414\u0438\u0435\u0433\u043E \u041C\u0430\u0440\u0430\u0434\u043E\u043D\u0430"
    LegendProgress:
      Club:
      - "\u0414\u0438\u0435\u0433\u043E \u0410\u0440\u043C\u0430\u043D\u0434\u043E
        \u041C\u0430\u0440\u0430\u0434\u043E\u043D\u0430 \u0440\u043E\u0434\u0438\u043B\u0441\u044F
        30 \u043E\u043A\u0442\u044F\u0431\u0440\u044F 1960 \u0433\u043E\u0434\u0430
        \u0432 \u043D\u0435\u0431\u043E\u043B\u044C\u0448\u043E\u043C \u0433\u043E\u0440\u043E\u0434\u043A\u0435
        \u041B\u0430\u043D\u0443\u0441, \u043A\u043E\u0442\u043E\u0440\u044B\u0439
        \u043D\u0430\u0445\u043E\u0434\u0438\u0442\u0441\u044F \u0432 \u043F\u0440\u043E\u0432\u0438\u043D\u0446\u0438\u0438
        \u0411\u0443\u044D\u043D\u043E\u0441-\u0410\u0439\u0440\u0435\u0441."
      - "\u041F\u0435\u0440\u0432\u044B\u043C \u043F\u0440\u043E\u0444\u0435\u0441\u0441\u0438\u043E\u043D\u0430\u043B\u044C\u043D\u044B\u043C
        \u043A\u043B\u0443\u0431\u043E\u043C \u041C\u0430\u0440\u0430\u0434\u043E\u043D\u044B
        \u0431\u044B\u043B \u0410\u0440\u0445\u0435\u043D\u0442\u0438\u043D\u043E\u0441
        \u0425\u0443\u043D\u0438\u043E\u0440\u0441, \u0437\u0430 \u043A\u043E\u0442\u043E\u0440\u044B\u0439
        \u043E\u043D \u0432\u044B\u0441\u0442\u0443\u043F\u0430\u043B \u0441 1976
        \u043F\u043E 1981 \u0433\u043E\u0434\u044B. \u0412 \u044D\u0442\u043E\u043C
        \u043A\u043B\u0443\u0431\u0435 \u043E\u043D \u043D\u0435 \u0432\u044B\u0438\u0433\u0440\u0430\u043B
        \u0442\u0440\u043E\u0444\u0435\u0435\u0432."
      - "\u0412 \u043D\u0430\u0447\u0430\u043B\u0435 1981 \u0433\u043E\u0434\u0430
        \u041C\u0430\u0440\u0430\u0434\u043E\u043D\u0430 \u043F\u0435\u0440\u0435\u0448\u0435\u043B
        \u0432 \u0411\u043E\u043A\u0430 \u0425\u0443\u043D\u0438\u043E\u0440\u0441,
        \u0441 \u043A\u043E\u0442\u043E\u0440\u044B\u043C \u0432\u044B\u0438\u0433\u0440\u0430\u043B
        \u0447\u0435\u043C\u043F\u0438\u043E\u043D\u0441\u0442\u0432\u043E \u0410\u0440\u0433\u0435\u043D\u0442\u0438\u043D\u044B."
      - "\u0412 1982 \u0433\u043E\u0434\u0443 \u041C\u0430\u0440\u0430\u0434\u043E\u043D\u0430
        \u043E\u0442\u043F\u0440\u0430\u0432\u0438\u043B\u0441\u044F \u0438\u0433\u0440\u0430\u0442\u044C
        \u0432 \u0415\u0432\u0440\u043E\u043F\u0435, \u043E\u043D \u043F\u043E\u0434\u043F\u0438\u0441\u0430\u043B
        \u043A\u043E\u043D\u0442\u0440\u0430\u043A\u0442 \u0441 \u043A\u0430\u0442\u0430\u043B\u043E\u043D\u0441\u043A\u043E\u0439
        \u0411\u0430\u0440\u0441\u0435\u043B\u043E\u043D\u043E\u0439. \u0412 \u0438\u0441\u043F\u0430\u043D\u0441\u043A\u043E\u043C
        \u043A\u043B\u0443\u0431\u0435 \u043E\u043D \u0437\u0430\u0432\u043E\u0435\u0432\u0430\u043B
        \u041A\u0443\u0431\u043E\u043A \u0418\u0441\u043F\u0430\u043D\u0438\u0438,
        \u041A\u0443\u0431\u043E\u043A \u041B\u0438\u0433\u0438, \u0430 \u0442\u0430\u043A\u0436\u0435
        \u0421\u0443\u043F\u0435\u0440\u043A\u0443\u0431\u043E\u043A \u0418\u0441\u043F\u0430\u043D\u0438\u0438."
      - "\u0412 1984 \u0433\u043E\u0434\u0443 \u0438\u0433\u0440\u043E\u043A \u043F\u0435\u0440\u0435\u0448\u0435\u043B
        \u0432 \u0438\u0442\u0430\u043B\u044C\u044F\u043D\u0441\u043A\u0438\u0439
        \u041D\u0430\u043F\u043E\u043B\u0438. \u0418\u0433\u0440\u0430\u044F \u0437\u0430
        \u043D\u0435\u0430\u043F\u043E\u043B\u0438\u0442\u0430\u043D\u0441\u043A\u0438\u0439
        \u043A\u043B\u0443\u0431, \u041C\u0430\u0440\u0430\u0434\u043E\u043D\u0430
        \u0434\u0432\u0430\u0436\u0434\u044B \u0432\u044B\u0438\u0433\u0440\u0430\u043B
        \u0447\u0435\u043C\u043F\u0438\u043E\u043D\u0441\u0442\u0432\u043E \u0441\u0442\u0440\u0430\u043D\u044B,
        \u041A\u0443\u0431\u043E\u043A \u0438 \u0421\u0443\u043F\u0435\u0440\u043A\u0443\u0431\u043E\u043A
        \u0418\u0442\u0430\u043B\u0438\u0438, \u0430 \u0442\u0430\u043A\u0436\u0435
        \u041A\u0443\u0431\u043E\u043A \u0423\u0435\u0444\u0430. \u0412 \u043A\u043B\u0443\u0431\u0435
        \u043E\u043D \u043F\u0440\u043E\u0432\u0435\u043B \u043E\u043A\u043E\u043B\u043E
        \u0441\u0435\u043C\u0438 \u043B\u0435\u0442."
      - "\u0417\u0430\u0442\u0435\u043C \u041C\u0430\u0440\u0430\u0434\u043E\u043D\u0430
        \u043E\u0442\u044B\u0433\u0440\u0430\u043B \u043E\u0434\u0438\u043D \u0441\u0435\u0437\u043E\u043D
        \u0437\u0430 \u0438\u0441\u043F\u0430\u043D\u0441\u043A\u0443\u044E \u0421\u0435\u0432\u0438\u043B\u044C\u044E,
        \u043D\u0435\u043C\u043D\u043E\u0433\u043E \u0437\u0430 \u0430\u0440\u0433\u0435\u043D\u0442\u0438\u043D\u0441\u043A\u0438\u0439
        \u041D\u044C\u044E\u044D\u043B\u043B\u0441 \u041E\u043B\u0434 \u0411\u043E\u0439\u0437
        \u0438 \u0435\u0449\u0435 \u043D\u0435\u0441\u043A\u043E\u043B\u044C\u043A\u043E
        \u043B\u0435\u0442 \u0437\u0430 \u0411\u043E\u043A\u0430 \u0425\u0443\u043D\u0438\u043E\u0440\u0441.
        \u041D\u043E \u0442\u0443\u0442 \u0443\u0436\u0435 \u0431\u0435\u0437 \u043A\u043B\u0443\u0431\u043D\u044B\u0445
        \u0442\u0440\u043E\u0444\u0435\u0435\u0432."
      NationalTeam:
      - "\u041D\u0430 \u0443\u0440\u043E\u0432\u043D\u0435 \u0441\u0431\u043E\u0440\u043D\u044B\u0445
        \u041C\u0430\u0440\u0430\u0434\u043E\u043D\u0430 \u0432\u044B\u0438\u0433\u0440\u0430\u043B
        \u043C\u043E\u043B\u043E\u0434\u0435\u0436\u043D\u044B\u0439 \u0427\u0435\u043C\u043F\u0438\u043E\u043D\u0430\u0442
        \u041C\u0438\u0440\u0430 (\u0434\u043E 20 \u043B\u0435\u0442), \u043E\u0441\u043D\u043E\u0432\u043D\u043E\u0439
        \u0427\u0435\u043C\u043F\u0438\u043E\u043D\u0430\u0442 \u041C\u0438\u0440\u0430
        (\u0438 \u0435\u0449\u0435 \u043E\u0434\u0438\u043D \u0440\u0430\u0437 \u0431\u044B\u043B
        \u0441\u0435\u0440\u0435\u0431\u0440\u044F\u043D\u044B\u043C \u043F\u0440\u0438\u0437\u0451\u0440\u043E\u043C)
        \u0438 \u043A\u0443\u0431\u043E\u043A \u0410\u0440\u0442\u0435\u043C\u0438\u043E
        \u0424\u0440\u0430\u043D\u043A\u0438."
      PersonalAchievements:
      - "\u0421\u0440\u0435\u0434\u0438 \u043E\u0441\u043E\u0431\u044B\u0445 \u0434\u043E\u0441\u0442\u0438\u0436\u0435\u043D\u0438\u0439
        \u0438\u0433\u0440\u043E\u043A\u0430 \u043C\u043E\u0436\u043D\u043E \u0432\u044B\u0434\u0435\u043B\u0438\u0442\u044C
        \u043F\u0440\u0438\u0441\u0443\u0436\u0434\u0435\u043D\u0438\u0435 \u043F\u043E\u0447\u0435\u0442\u043D\u043E\u0433\u043E
        \u0417\u043E\u043B\u043E\u0442\u043E\u0433\u043E \u041C\u044F\u0447\u0430,
        \u043F\u0440\u0438\u0437\u043D\u0430\u043D\u0438\u0435 \u0430\u0432\u0442\u043E\u0440\u043E\u043C
        \u043B\u0443\u0447\u0448\u0435\u0433\u043E \u0433\u043E\u043B\u0430 \u0432
        \u0438\u0441\u0442\u043E\u0440\u0438\u0438 \u0444\u0443\u0442\u0431\u043E\u043B\u0430
        \u0438 \u043F\u0440\u0438\u0437\u043D\u0430\u043D\u0438\u0435 \u043B\u0443\u0447\u0448\u0438\u043C
        \u0438\u0433\u0440\u043E\u043A\u043E\u043C \u043F\u0440\u043E\u0448\u043B\u043E\u0433\u043E
        \u0441\u0442\u043E\u043B\u0435\u0442\u0438\u044F."
